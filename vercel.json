{
  "version": 2,
  "buildCommand": "npx ng build --configuration=production",
  "outputDirectory": "dist/app-angular",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/tmdb.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}